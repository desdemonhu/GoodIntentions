            <kind>1</kind>
            <string>///Dialogue
var itemCheck;

if keyboard_check_released(ord("B")) {
        dialogue_foxFigurePuzzle = clamp(dialogue_foxFigurePuzzle-2, 0,30);
        dialoguechainPuzzle = clamp(dialogue_foxFigurePuzzle-2, 0,30);
        
        rewind = true;
    }
    
if(mouse_check_button_released(mb_left)|| keyboard_check_pressed(vk_enter) || rewind)
{   
    rewind = false;
    
    if (foxFigurePuzzle){
    var str;
    var strName;
    switch (dialogue_foxFigurePuzzle)
    {
        case 0:
            str = "You have Combined the fox with the sink";
            strName = mcFirstName;
            //show_grace = true;
           ///show_loren = false;
            //show_meredith = false;
            mouseItem = -1;
            ++dialogue_foxFigurePuzzle;
            with (obj_text){
                instance_destroy();
                }
            break;
        case 1:
            str = "Rad";
            strName = "Tao";
            
            ++dialogue_foxFigurePuzzle;
            with (obj_text){
                instance_destroy();
                }
            break;
        case 2: 
            str = "This is some more dialogue";
            strName = mcFirstName;
            ++dialogue_foxFigurePuzzle;
            with (obj_text){
                instance_destroy();
                }
            break;
        case 3: 
            str = "About the puzzle";
             strName = "Tao";
            ++dialogue_foxFigurePuzzle; 
            with (obj_text){
                instance_destroy();
                }
            break;
        case 4: 
            str = "And you get a pipe out of it";
            strName = mcFirstName;
            
            ///Get pipe
            itemCheck = scr_itemcheck(2)
            if(itemCheck == 0){scr_itempickup(2);}
            
            ++dialogue_foxFigurePuzzle;
            with (obj_text){
                instance_destroy();
                }                       
            break;
        case 5:
            str = "Extra rad!";
            strName = "Tao";
            foxFigurePuzzle = false;
            //with obj_parentChar {
            //    event_perform(ev_collision, obj_parentEnemy);
            //}
            with (obj_text){
                instance_destroy();
                } 
            break;
        default: str = "Something went wrong..."; break;
    }
    scr_text(str,0.5,200, 600,strName);
}

else if(chainPuzzle){
    var str;
    var strName;
    
    switch(dialogue_chainPuzzle){
        case 0:
            str = "You have Combined the pipe with the bench";
            strName = mcFirstName;
            //show_grace = true;
           ///show_loren = false;
            //show_meredith = false;
            mouseItem = -1;
            
            ///Put pipe back in the inventory
            var itemCheck = scr_itemcheck(2);
            if(itemCheck == 0){scr_itempickup(2);}
            
            ++dialogue_chainPuzzle;
            with (obj_text){
                instance_destroy();
                }
            break;
            
            case 1:
            str = "Extra rad! You get the chain now!";
            strName = "Tao";
            chainPuzzle = false;

            var itemCheckChain = scr_itemcheck(5);
            if(itemCheckChain == 0){scr_itempickup(5)};

            with (obj_text){
                instance_destroy();
                } 
            break;
        default: str = "Something went wrong..."; break;
    }
    scr_text(str,0.5,200, 600,strName);
}

